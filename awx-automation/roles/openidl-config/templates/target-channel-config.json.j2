{
    "targetChannels": [
{% for channel in network.channels %}
{% if channel.name == "defaultchannel" or org_name in channel.name %}
        {
            "chaincodeName": "{{ channel.chaincode.name }}",
            "channelName": "{{ channel.name }}"
        }{% if loop.last %}{% else %},{% endif %}
{% endif %}
{% endfor %}
    ],
    "users": [
        {
            "mspId": "{{ network.org_name }}MSP",
            "org": "{{ network.org_name }}",
            "user": "openidl-{{ network.org_name }}-data-call-processor-ibp-2.0"
        }
    ]
}