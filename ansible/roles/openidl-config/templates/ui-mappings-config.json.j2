{
    "data-call-app-url": {
        "env": "DATA_CALL_APP_URL",
        "value": "https://{{app_data_call_address}}/openidl/api"
    },
    "idp-credentials": {
        "env": "IDP_CONFIG",
        "file": "{{idp_config_file}}"
    },
    "log-level": {
        "env": "LOG_LEVEL",
        "value": "debug"
    },
    "port": {
        "env": "PORT",
        "value": "8080"
    },
    "icon-bucket-url": {
		"value": "https://{{ s3_icon_bucket_url }}/",
		"env": "ICON_BUCKET_URL"
	},
    "off-chain-db-credentials": {
        "env": "OFF_CHAIN_DB_CONFIG",
        "file": "local-db-config.json"
    }
}